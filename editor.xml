<application>
  <component name="DaemonCodeAnalyzerSettings">
    <option name="SHOW_METHOD_SEPARATORS" value="true" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_FAMILY" value="Monaco" />
    <option name="LINE_SPACING" value="1.5" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="JAVA" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR:CONSOLE" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc;*.java;*.class;*.xml;*.properties;*.yml" />
    <option name="SHOW_BREADCRUMBS_ABOVE" value="true" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\b(todo|fixme)\b.*" />
  </component>
</application>